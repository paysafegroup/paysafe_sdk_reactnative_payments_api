{
  "name": "demo-app-expo",
  "root": "DemoAppExpo",
  "sourceRoot": "DemoAppExpo/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/expo:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/DemoAppExpo",
        "platform": "all"
      }
    },
    "start": {
      "executor": "@nx/expo:start",
      "options": {
        "port": 8081
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "DemoAppExpo/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["DemoAppExpo/**/*.{ts,tsx,js,jsx}"]
      }
    }
  }
}
